#include <stdio.h>
//alphabetic pattern 
int main() {
   ;
   int i, j, n;
   scanf("%d",&n);
  for(i=1; i<=n; i++){
    
      for (j=1; j<=i ; j++){
           
          printf("%c",j+64);
      }
      printf("\n");
  }
    return 0;
}
//alphabetic pattern row is printing and lower case
#include <stdio.h>

int main() {
   ;
   int i, j, n;
   scanf("%d",&n);
  for(i=1; i<=n; i++){
    
      for (j=1; j<=i ; j++){
           
          printf("%c",i+64);
      }
      printf("\n");
  }
    return 0;
}
